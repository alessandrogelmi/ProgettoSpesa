  <script type="text/javascript">
    document.getElementById("Categories").onclick = function () {
      if (document.getElementById("Fruit").style.display == "list-item") {
        document.getElementById("Fruit").style.display = "none";
        document.getElementById("Cheese").style.display = "none";
        document.getElementById("Hamburger").style.display = "none";
        document.getElementById("Egg").style.display = "none";
        document.getElementById("Fish").style.display = "none";
        document.getElementById("Soap").style.display = "none";
        document.getElementById("Arrow").style.transition = "600ms";
        document.getElementById("Arrow").style.transform = "rotate(-360deg)";
      } else {
        document.getElementById("Fruit").style.display = "list-item";
        document.getElementById("Cheese").style.display = "list-item";
        document.getElementById("Hamburger").style.display = "list-item";
        document.getElementById("Egg").style.display = "list-item";
        document.getElementById("Fish").style.display = "list-item";
        document.getElementById("Soap").style.display = "list-item";
        document.getElementById("Arrow").style.transition = " 600ms";
        document.getElementById("Arrow").style.transform = "rotate(90deg)";
      }
    };
